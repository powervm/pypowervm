[project]
	description = REST Python Wrapper (K2operator)
[access "refs/*"]
	owner = group Administrators
	owner = group Gerrit Management
[access "refs/for/refs/heads/*"]
	push = group pypowervm Developer
	push = group pypowervm Integrator
	pushMerge = group pypowervm Developer
	pushMerge = group pypowervm Integrator
	forgeCommitter = group pypowervm Integrator
[access "refs/heads/*"]
	forgeAuthor = group pypowervm Developer
	forgeAuthor = group pypowervm Integrator
	label-Code-Review = -2..+2 group pypowervm Integrator
	label-Code-Review = -2..+1 group pypowervm Developer
	label-Code-Review = -1..+1 group pypowervm Reviewer
	label-Verified = -1..+1 group pypowervm Developer
	label-Verified = -1..+1 group pypowervm Integrator
	read = group pypowervm Developer
	read = group pypowervm Integrator
	read = group pypowervm Reviewer
	submit = group pypowervm Developer
	submit = group pypowervm Integrator
	create = group pypowervm Integrator
	push = group pypowervm Integrator
	pushMerge = group pypowervm Integrator
	pushTag = group pypowervm Integrator
[access "refs/meta/config"]
	read = group pypowervm Integrator
[access "refs/tags/*"]
	read = group pypowervm Reviewer
