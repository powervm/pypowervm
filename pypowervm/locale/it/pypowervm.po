#: pypowervm/exceptions.py:77
msgid "The virtual adapter configuration is invalid."
msgstr "La configurazione dell'adattatore virtuale non è valida."

#: pypowervm/exceptions.py:81
#, python-format
msgid "%(attr)s is a required attribute for virtual machine '%(instance_name)s'."
msgstr "%(attr)s è un attributo richiesto per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:86
#, python-format
msgid ""
"'%(value)s' is not a valid value for %(attr)s for virtual machine "
"'%(instance_name)s'."
msgstr ""
"'%(value)s' non è un valore valido per %(attr)s per la macchina virtuale "
"'%(instance_name)s'."

#: pypowervm/exceptions.py:91
#, python-format
msgid ""
"The value for minimum %(attr)s (%(value)s) cannot be greater than the "
"desired %(attr)s (%(des_value)s) for virtual machine '%(instance_name)s'."
msgstr ""
"Il valore per %(attr)s minimo (%(value)s) non può essere superiore al "
"%(attr)s (%(des_value)s) desiderato per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:97
#, python-format
msgid ""
"The value for maximum %(attr)s (%(value)s) cannot be less than the "
"desired %(attr)s (%(des_value)s) for virtual machine '%(instance_name)s'."
msgstr ""
"Il valore per %(attr)s massimo (%(value)s) non può essere inferiore al "
"%(attr)s desiderato (%(des_value)s) per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:103
#, python-format
msgid ""
"If you specify a minimum or maximum value for %(attr)s, you must specify "
"both minimum and maximum %(attr)s for virtual machine "
"'%(instance_name)s'."
msgstr ""
"Se si specifica un valore minimo o massimo per %(attr)s, è necessario specificare "
"%(attr)s minimo e massimo per la macchina virtuale "
"'%(instance_name)s'."

#: pypowervm/exceptions.py:109
#, python-format
msgid ""
"%(attr)s (%(value)d MB) is not a multiple of the region memory size "
"(%(lmb)d MB) for virtual machine '%(instance_name)s'. Specify a value "
"that is a multiple of the region memory size."
msgstr ""
"%(attr)s (%(value)d MB) non è un multiplo della dimensione della memoria dell'area "
"(%(lmb)d MB) per la macchina virtuale '%(instance_name)s'. Specificare un valore "
"che sia un multiplo della dimensione della memoria dell'area."

#: pypowervm/exceptions.py:116
#, python-format
msgid ""
"'%(availability_priority)s' is not a valid value for availability "
"priority for virtual machine '%(instance_name)s'. Specify a value between"
" 0 and 255."
msgstr ""
"'%(availability_priority)s' non è un valore valido per la priorità "
"della disponibilità per la macchina virtuale '%(instance_name)s'. Specificare un valore compreso"
" tra 0 e 255."

#: pypowervm/exceptions.py:122
#, python-format
msgid ""
"'%(processor_compatibility)s' is not a valid option for processor "
"compatibility for virtual machine '%(instance_name)s'. Valid options "
"include the following: '%(valid_values)s'."
msgstr ""
"'%(processor_compatibility)s' non è un'opzione valida per la compatibilità "
"del processore per la macchina virtuale '%(instance_name)s'. Opzioni valide "
"includono le seguenti: '%(valid_values)s'."

#: pypowervm/exceptions.py:129
#, python-format
msgid ""
"Changing '%(processor_compatibility)s' is not supported because for an "
"IBM i virtual machine '%(instance_name)s', its processor compatibility "
"mode can only be set as 'default' on Power6 host."
msgstr ""
"La modifica di '%(processor_compatibility)s' non è supportata a causa di "
"una macchina virtuale IBM i '%(instance_name)s', la relativa modalità di "
"compatibilità processore può essere impostata solo come 'default' sll'host Power6."

#: pypowervm/exceptions.py:137
#, python-format
msgid ""
"'%(value)s' is not valid for virtual machine '%(instance_name)s'. "
"%(option)s must either be true or false. "
msgstr ""
"'%(value)s' non è valido per la macchina virtuale '%(instance_name)s'. "
"%(option)s deve essere true o false. "

#: pypowervm/exceptions.py:143
#, python-format
msgid ""
"'%(ded_share_mode)s' is not a valid option for sharing mode for dedicated"
" processors for virtual machine '%(instance_name)s'. Valid options "
"include the following: '%(valid_values)s'."
msgstr ""
"'%(ded_share_mode)s' non è un'opzione valida per la modalità di condivisione per i"
" processori dedicati per la macchina virtuale '%(instance_name)s'. Le opzioni valide "
"sono le seguenti: '%(valid_values)s'."

#: pypowervm/exceptions.py:150
#, python-format
msgid ""
"'%(option)s' is not a valid option for dedicated processor mode for "
"virtual machine '%(instance_name)s'."
msgstr ""
"'%(option)s' non è un'opzione valida per la modalità processore dedicato per "
"la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:155
#, python-format
msgid ""
"'%(option)s' is not a valid option for shared processor mode for virtual "
"machine '%(instance_name)s'."
msgstr ""
"'%(option)s' non è un'opzione valida per la modalità processore condiviso per "
"la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:160
#, python-format
msgid ""
"Sharing weight is not a valid option for capped sharing mode for virtual "
"machine '%(instance_name)s'."
msgstr ""
"La condivisione del peso non è un'opzione valida per la modalità di condivisione limitata per "
"la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:165
#, python-format
msgid ""
"'%(shared_weight)s' is not a valid value for shared processor weight for "
"virtual machine '%(instance_name)s'. Specify a value between 0 and 255."
msgstr ""
"'%(shared_weight)s' non è un valore valido per il peso del processore condiviso per "
"la macchina virtuale '%(instance_name)s'. Specificare un valore compreso tra 0 e 255."

#: pypowervm/exceptions.py:171
#, python-format
msgid ""
"If you specify a minimum and maximum value for processing units, you must"
" also specify a minimum and maximum value for virtual CPUs for virtual "
"machine '%(instance_name)s'."
msgstr ""
"Se si specifica un valore minimo e massimo per le unità di elaborazione, è necessario"
" specificare anche un valore minimo e massimo per le CPU virtuali per "
"la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:177
#, python-format
msgid ""
"If you specify a minimum and maximum value for processing units, you must"
" also specify a value for desired processing units for virtual machine "
"'%(instance_name)s'."
msgstr ""
"Se si specifica un valore minimo e massimo per le unità di elaborazione, è necessario"
" specificare anche un valore per le unità di elaborazione desiderate per la macchina virtuale "
"'%(instance_name)s'."

#: pypowervm/exceptions.py:183
#, python-format
msgid ""
"There must be a minimum of %(proc_units_per_vcpu)s processing units per "
"virtual CPU. The requested %(level)s processing units were "
"'%(proc_units)s' and the requested %(level)s virtual CPUs were "
"'%(vcpus)d' for virtual machine '%(instance_name)s'."
msgstr ""
"Deve esserci un minimo di %(proc_units_per_vcpu)s unità di elaborazione per "
"CPU virtual. Le unità di elaborazione %(level)s richieste erano "
"'%(proc_units)s' e le CPU virtuali %(level)s richieste erano "
"'%(vcpus)d' per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:191
#, python-format
msgid ""
"The number of processing units cannot be a larger value than the number "
"of virtual CPUs. The requested %(level)s processing units were "
"'%(proc_units)s' and the requested %(level)s virtual CPUs were "
"'%(vcpus)d' for virtual machine '%(instance_name)s'."
msgstr ""
"Il numero di unità di elaborazione non può essere un valore maggiore del numero "
"di CPU virtuali. Le unità di elaborazione %(level)s richieste erano "
"'%(proc_units)s' e le CPU virtuali %(level)s richieste erano "
"'%(vcpus)d' per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:199
#, python-format
msgid ""
"The desired processors (%(vcpus)d) cannot be above the maximum processors"
" allowed per partition (%(max_allowed)d) for virtual machine "
"'%(instance_name)s'."
msgstr ""
"I processori desiderati (%(vcpus)d) non possono essere più del numero massimo di processori"
" consentiti per partizione (%(max_allowed)d) per la macchina virtuale "
"'%(instance_name)s'."

#: pypowervm/exceptions.py:205
#, python-format
msgid ""
"The maximum processors (%(vcpus)d) cannot be above the maximum system "
"capacity processor limit %(max_allowed)d) for virtual machine "
"'%(instance_name)s'."
msgstr ""
"Il numero massimo di processori (%(vcpus)d) non può essere superiore al limite massimo "
"di processori della capacità di sistema %(max_allowed)d) per la macchina virtuale "
"'%(instance_name)s'."

#: pypowervm/exceptions.py:211
#, python-format
msgid ""
"Unable to resize virtual machine '%(instance_name)s'. Resizing an active "
"virtual machine requires RMC connectivity."
msgstr ""
"Impossibile modificare le dimensioni della macchina virtuale '%(instance_name)s'. Il ridimensionamento "
"di una macchina virtuale attiva richiede la connettività RMC."

#: pypowervm/exceptions.py:217
#, python-format
msgid ""
"Unable to determine that the volume with id %(uuid)s has been discovered "
"or mapped on a Virtual I/O Server"
msgstr ""
"Impossibile determinare se il volume con id %(uuid)s è stato rilevato "
"o associato su un VIOS (Virtual I/O Server)"

#: pypowervm/exceptions.py:222
msgid "Unable to locate the Virtual I/O Server for this operation"
msgstr "Impossibile individuare il VIOS (Virtual I/O Server) per questa operazione"

#: pypowervm/exceptions.py:226
#, python-format
msgid ""
"The storage type specified '%(storage_type)s' is not supported for this "
"operation"
msgstr ""
"Il tipo di memoria specificato '%(storage_type)s' non è supportato per questa "
"operazione"

#: pypowervm/exceptions.py:231
#, python-format
msgid ""
"Desired %(attr)s requested (%(desired)s) is outside the currently "
"assigned minimum (%(min)s) and maximum (%(max)s) values for virtual "
"machine '%(instance_name)s'. Either specify a valid value for desired "
"%(attr)s or power off the virtual machine before resizing."
msgstr ""
"%(attr)s desiderato richiesto (%(desired)s) è al di fuori dei "
"valori minimo e (%(min)s) e massimo (%(max)s) assegnati per "
"la macchina virtuale '%(instance_name)s'. Specificare un valore valido per "
"%(attr)s desiderato o spegnere la macchina virtuale prima del ridimensionamento."

#: pypowervm/exceptions.py:239
#, python-format
msgid ""
"Changing %(attr)s is not supported while virtual machine "
"'%(instance_name)s' is running. The virtual machine must be powered off "
"first."
msgstr ""
"La modifica di %(attr)s non è supportata mentre la macchina virtuale "
"'%(instance_name)s' è in esecuzione. La macchina virtuale deve essere spenta "
"prima."

#: pypowervm/exceptions.py:245
#, python-format
msgid ""
"Failed to update object properties for %(uuid)s because the object "
"information is out of date."
msgstr ""
"Aggiornamento delle proprietà oggetto per %(uuid)s non riuscito perché le "
"informazioni oggetto sono obsolete."

#: pypowervm/exceptions.py:250
#, python-format
msgid ""
"Unable to find a supported storage connection type in the list of storage"
" connectivity groups: %(scg_conn_types)s"
msgstr ""
"Impossibile trovare un tipo di connessione di memoria supportato nell'elenco di"
" gruppi di connettività di memoria: %(scg_conn_types)s"

#: pypowervm/exceptions.py:255
#, python-format
msgid ""
"Unable to create virtual adapter for attaching storage to virtual machine"
" %(instance_name)s. Reason: %(reason)s"
msgstr ""
"Impossibile creare l'adattatore virtuale per il collegamento della memoria alla macchina virtuale"
" %(instance_name)s. Motivo: %(reason)s"

#: pypowervm/exceptions.py:260
#, python-format
msgid ""
"Unable to perform this operation with the storage connectivity group "
"provided for virtual machine %(instance_name)s."
msgstr ""
"Impossibile eseguire questa operazione con il gruppo di connettività di memoria "
"fornito per la macchina virtuale %(instance_name)s."

#: pypowervm/exceptions.py:266
#, python-format
msgid ""
"Failed to update object properties for %(uuid)s because the object "
"information is incomplete."
msgstr ""
"Aggiornamento delle proprietà oggetto per %(uuid)s non riuscito perché le "
"informazioni oggetto sono incomplete."

#: pypowervm/exceptions.py:271
#, python-format
msgid ""
"Unable to configure storage for virtual machine '%(instance_name)s'. "
"Configuring storage for a virtual machine in Active state requires RMC "
"connectivity."
msgstr ""
"Impossibile configurare la memoria per la macchina virtuale '%(instance_name)s'. "
"La configurazione della memoria per una macchina virtuale in stato Attivo richiede la "
"connettività RMC."

#: pypowervm/exceptions.py:277
#, python-format
msgid ""
"Invalid attribute name '%(key)s' was passed in as part of the flavor "
"extra specs for virtual machine '%(instance_name)s'."
msgstr ""
"Nome attributo non valido '%(key)s' passato come parte delle "
"specifiche extra della versione per la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:283
msgid ""
"The operation cannot complete because the system is busy. Wait a few "
"minutes and then try the operation again."
msgstr ""
"L'operazione non può essere completata perché il sistema è occupato. Attendere "
"qualche minuto ed eseguire nuovamente l'operazione."

#: pypowervm/exceptions.py:288
#, python-format
msgid ""
"Unable to find volume group '%(volume_group_name)s' on Virtual I/O Server"
" '%(uuid)s'."
msgstr ""
"Impossibile trovare il gruppo di volumi '%(volume_group_name)s' sul Virtual I/O Server"
" '%(uuid)s'."

#: pypowervm/exceptions.py:293
#, python-format
msgid "Unable to retrieve host statistics for managed host %(host)s."
msgstr "Impossibile richiamare le statistiche host per l'host gestito %(host)s."

#: pypowervm/exceptions.py:298
#, python-format
msgid ""
"Unable to remove the virtual adapter from Virtual I/O Server %(uuid)s "
"because it is a required adapter."
msgstr ""
"Impossibile rimuovere l'adattatore virtuale dal Virtual I/O Server %(uuid)s "
"perché è un adattatore richiesto."

#: pypowervm/exceptions.py:303
#, python-format
msgid ""
"Unable to create virtual adapters on Virtual I/O Server '%(uuid)s'. Run "
"the Verify Environment tool. You can either click the Verify Environment "
"button on the GUI Home page or type the powervc-validate command on the "
"command line. Verify that a sufficient number of virtual resources are "
"configured for the Virtual I/O Server. Then, try the operation again."
msgstr ""
"Impossibile creare gli adattatori virtuali sul VIOS (Virtual I/O Server) '%(uuid)s'. Eseguire "
"lo strumento di verifica dell'ambiente. È possibile fare clic sul pulsante Verifica ambiente "
"sulla Home page della GUI o immettere il comando powervc-validate sulla "
"riga comandi. Verificare che sia configurato un numero sufficiente di risorse virtuali "
"per il VIOS (Virtual I/O Server). Tentare quindi nuovamente l'operazione."

#: pypowervm/exceptions.py:313
#, python-format
msgid "The license for Virtual I/O Server %(vios_name)s has not been accepted."
msgstr "La licenza per il VIOS %(vios_name)s non è stata accettata."

#: pypowervm/exceptions.py:318
#, python-format
msgid ""
"Virtual I/O Server %(vios_name)s is not running. The current state is: "
"%(vios_state)s"
msgstr ""
"Il Virtual I/O Server %(vios_name)s non è in esecuzione. Lo stato corrente è: "
"%(vios_state)s"

#: pypowervm/exceptions.py:323
#, python-format
msgid ""
"Virtual I/O Server %(vios_name)s does not have an active Resource "
"Monitoring Control state."
msgstr ""
"Il Virtual I/O Server %(vios_name)s non ha uno stato RMC "
"(Resource Monitoring Control) attivo."

#: pypowervm/exceptions.py:328
#, python-format
msgid ""
"Virtual I/O Server %(vios_name)s is currently too busy to process update "
"operations."
msgstr ""
"Virtual I/O Server %(vios_name)s attualmente è troppo occupato per elaborare le "
"operazioni di aggiornamento."

#: pypowervm/exceptions.py:333
#, python-format
msgid ""
"Virtual I/O Server %(vios_name)s does not have any fibre channel ports "
"listed in the storage connectivity group."
msgstr ""
"Il Virtual I/O Server %(vios_name)s non dispone di alcuna porta fibre channel "
"elencata nel gruppo di connettività di memoria."

#: pypowervm/exceptions.py:339
#, python-format
msgid ""
"There are not enough Virtual I/O Servers with the necessary connectivity "
"within the storage connectivity group for virtual machine "
"'%(instance_name)s' volume configuration: (Requested: %(min_vios)s "
"Available: %(avail_vios)s). %(reasons)s"
msgstr ""
"Non sono presenti Virtual I/O Server sufficienti con la connettività necessaria "
"all'interno del gruppo di connettività di memoria per la configurazione volume "
"'della macchina virtuale %(instance_name)s': (Richiesti: %(min_vios)s "
"Disponibili: %(avail_vios)s). %(reasons)s"

#: pypowervm/exceptions.py:347
#, python-format
msgid ""
"Unable to configure storage adapters for virtual machine "
"%(instance_name)s on the required number of Virtual I/O Servers for the "
"volume. Minimum number of Virtual I/O Servers required per volume: "
"%(min_vios)s. %(reasons)s"
msgstr ""
"Impossibile configurare adattatori di memoria per la macchina virtuale "
"%(instance_name)s sul numero richiesto di Virtual I/O Servers per il "
"volume. Numero minimo di VIOS richiesto per volume: "
"%(min_vios)s. %(reasons)s"

#: pypowervm/exceptions.py:355
#, python-format
msgid ""
"Unable to configure storage adapters for virtual machine "
"%(instance_name)s on available physical Fibre Channel ports that satisfy "
"the NPIV switch fabric requirement of the storage connectivity group. "
"%(reasons)s"
msgstr ""
"Impossibile configurare adattatori di memoria per la macchina virtuale "
"%(instance_name)s sulle porte Fibre Channel fisiche disponibili che soddisfino "
"il requisito fabric switch NPIV del gruppo di connettività di memoria. "
"%(reasons)s"

#: pypowervm/exceptions.py:362
#, python-format
msgid ""
"Unable to derive the appropriate physical FC port for WWPN %(wwpn)s.  The"
" VIOS Extended Attribute Groups may have been insufficient.  The VIOS URI"
" for the query was %(vio_uri)s."
msgstr ""
"Impossibile derivare la porta FC fisica appropriata per WWPN %(wwpn)s.  I"
" gruppi di attributi estesi VIOS potrebbero essere stati insufficienti.  L'URI VIOS"
" per la query era %(vio_uri)s."

#: pypowervm/exceptions.py:368
#, python-format
msgid ""
"The virtual machine %(name)s cannot use the specified port because it "
"would require MAC address %(mac)s to be set. The host does not support "
"custom MAC addresses."
msgstr ""
"La macchina virtuale %(name)s non può utilizzare la porta specificata perché "
"richiederebbe l'impostazione dell'indirizzo MAC %(mac)s. L'host non supporta "
"gli indirizzi MAC personalizzati."

#: pypowervm/exceptions.py:374
msgid ""
"The operation failed because it is only allowed when the managed host is "
"in the Standby or Operating state."
msgstr ""
"L'operazione non è riuscita perché è consentita solo quando l'host gestito è "
"in stato Standby o Operativo."

#: pypowervm/exceptions.py:379
msgid ""
"Error getting information from the Virtual I/O Server(s). This could be "
"because the it is down or too busy to process the request."
msgstr ""
"Errore durante l'acquisizione delle informazioni dai Virtual I/O Server. Potrebbe essere "
"perché è inattivo o troppo occupato per elaborare la richiesta."

#: pypowervm/exceptions.py:385
#, python-format
msgid "Element not found: %(element_type)s %(element)s"
msgstr "Elemento non trovato: %(element_type)s %(element)s"

#: pypowervm/exceptions.py:389
#, python-format
msgid "LPAR not found: %(lpar_name)s"
msgstr "LPAR non trovata: %(lpar_name)s"

#: pypowervm/exceptions.py:393
#, python-format
msgid "System not found: %(system_name)s"
msgstr "Sistema non trovato: %(system_name)s"

#: pypowervm/exceptions.py:397
#, python-format
msgid "The migration task failed. %(error)s"
msgstr "Attività di migrazione non riuscita. %(error)s"

#: pypowervm/exceptions.py:401
#, python-format
msgid "Migration of %(lpar_name)s is already in progress."
msgstr "La migrazione di %(lpar_name)s è già in corso."

#: pypowervm/exceptions.py:405
#, python-format
msgid "Unable to update progress for virtual machine '%(uuid)s'."
msgstr "Impossibile aggiornare l'avanzamento per la macchina virtuale '%(uuid)s'."

#: pypowervm/exceptions.py:409
#, python-format
msgid ""
"The media library is too small for the ISO image. The media library "
"requires %(medialib_mb_required)d MB of space but only "
"%(mediavg_mb_avail)d MB is available."
msgstr ""
"La libreria di supporti è troppo piccola per l'immagine ISO. La libreria di supporti "
"richiede %(medialib_mb_required)d MB di spazio ma solo "
"%(mediavg_mb_avail)d MB sono disponibili."

#: pypowervm/exceptions.py:415
#, python-format
msgid ""
"Virtual machine '%(instance_name)s' is running during capture. Virtual "
"machines must be stopped before they can be captured."
msgstr ""
"La macchina virtuale '%(instance_name)s' è in esecuzione durante l'acquisizione. Le macchine virtuali "
"devono essere arrestate prima di poter essere acquisite."

#: pypowervm/exceptions.py:421
#, python-format
msgid "Unable to find virtual machine '%(instance_name)s'."
msgstr "Impossibile trovare la macchina virtuale '%(instance_name)s'."

#: pypowervm/exceptions.py:425
msgid "Insufficient resources"
msgstr "Risorse insufficienti"

#: pypowervm/exceptions.py:429
#, python-format
msgid ""
"The requested memory (%(mem_requested)d MB) is below the minimum required"
" value (%(mem_min)d MB)"
msgstr ""
"La memoria richiesta (%(mem_requested)d MB) è inferiore al valore minimo"
" richiesto (%(mem_min)d MB)"

#: pypowervm/exceptions.py:434
#, python-format
msgid ""
"The requested disk (%(new_disk_size)d GB) is lesser than the existing "
"disk (%(disk_size)d GB)"
msgstr ""
"Il disco richiesto (%(new_disk_size)d GB) è inferiore al "
"disco esistente (%(disk_size)d GB)"

#: pypowervm/exceptions.py:439
#, python-format
msgid ""
"The requested CPUs (%(cpus_requested)d) are below the minimum required "
"value (%(cpus_min)d) "
msgstr ""
"Le CPU richieste (%(cpus_requested)d) sono inferiori al valore minimo "
"richiesto (%(cpus_min)d) "

#: pypowervm/exceptions.py:444
#, python-format
msgid ""
"The requested processing units (%(units_requested)d) are below the "
"minimum required value (%(units_min)d) "
msgstr ""
"Le unità di elaborazione richieste (%(units_requested)d) sono inferiori al "
"valore minimo richiesto (%(units_min)d) "

#: pypowervm/exceptions.py:449
#, python-format
msgid ""
"Insufficient free memory: (%(mem_requested)d MB requested, %(mem_avail)d "
"MB free)"
msgstr ""
